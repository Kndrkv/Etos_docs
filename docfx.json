{
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "О платформе/**.md",
          "О платформе/**/toc.yml",
          "Описание интерфейса/**.md",
          "Описание интерфейса/**/toc.yml",
          "Видео/**.md",
          "Видео/**/toc.yml",
          "Настройка пресетов/**.md",
          "Настройка пресетов/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appTitle": "ЭТОС",
      "_enableSearch": true
    },
    "fileMetadataFiles": [],
    "template": [
      "default", //"statictoc"
      "templates/myTheme"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
